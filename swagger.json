  "paths": {
    "/Register": {
      "post": {
        "summary": "Register a new user",
        "description": "Create a new user with their name, email, and password.",
        "parameters": [
          {
            "in": "body",
            "name": "user",
            "description": "The user to create.",
            "schema": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "email": {
                  "type": "string"
                },
                "password": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "201": {
            "description": "User Successfully Registered"
          },
          "400": {
            "description": "Bad Request (email already exists)"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
